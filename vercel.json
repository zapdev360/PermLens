{
  "version": 2,
  "outputDirectory": "frontend/dist",
  "functions": {
    "api/[...path].js": {
      "includeFiles": "shared/**,frontend/public/favicon.svg,frontend/public/favicon.ico"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/[...path].js" },
    { "src": "/(.*)", "dest": "/api/[...path].js" }
  ]
}